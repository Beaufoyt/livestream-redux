{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "rules": {
    "strict": 0,
    "max-len": [1, 120, 2, {
      "ignoreComments": true
    }],
    "react/jsx-indent-props": [1, 4],
    "react/jsx-indent": [1, 4],
    "indent": [1, 4],
    "no-console": 0,
    "import/no-dynamic-require": 0,
    "no-unexpected-multiline": 1,
    "no-unused-vars": 1,
    "arrow-body-style": 0,
  },
  "env": {
    "browser": true
  },
  "globals": {
    "__DEV__"      : false,
    "__PROD__"     : false,
    "__DEBUG__"    : false,
    "__DEBUG_NW__" : false,
    "BACKEND": true
  }
}
